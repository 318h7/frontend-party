{
  "compilerOptions": {
    "outDir": "dist/",
    "baseUrl": "src",
    "strict": true,
    "skipLibCheck": true,
    "esModuleInterop": true,
    "noImplicitAny": true,
    "noImplicitReturns": true,
    "noImplicitThis": true,
    "noUnusedLocals": true,
    "sourceMap": true,
    "module": "commonjs",
    "target": "es5",
    "jsx": "react",
    "allowJs": true,
    "paths": {
      "test-utils": ["../test/test-utils"],
      "axios-utils": ["../test/axios-utils"]
    }
  }
}
