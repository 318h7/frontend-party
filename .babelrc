{
    "presets": [
      [
        "@babel/preset-env",
        {
          "useBuiltIns": "entry",
          "targets": "> 0.25%, not dead"
       }
      ],
      "@babel/preset-typescript",
      "@babel/preset-react"
    ],
    "env": {
      "production": {
        "plugins": [
          ["react-remove-properties", {"properties": ["data-qa"]}],
          ["babel-plugin-styled-components"]
        ]
      }
    }
}
